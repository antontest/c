!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
age	t.c	/^    int age;$/;"	m	struct:stu	file:	access:public
id	t.c	/^    int id;$/;"	m	struct:stu	file:	access:public
main	t.c	/^int main( int argc, char **argv ) {$/;"	f	signature:( int argc, char **argv )
name	t.c	/^    char* name;$/;"	m	struct:stu	file:	access:public
stu	t.c	/^struct stu {$/;"	s	file:
stu::age	t.c	/^    int age;$/;"	m	struct:stu	file:	access:public
stu::id	t.c	/^    int id;$/;"	m	struct:stu	file:	access:public
stu::name	t.c	/^    char* name;$/;"	m	struct:stu	file:	access:public
